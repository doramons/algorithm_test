    "num = int(input())\n",
    "c = 1\n",
    "if num == 1:\n",
    "    print(1)\n",
    "else:\n",
    "    for i in range(1,num):\n",
    "        c += 6*i\n",
    "        if num <= c:\n",
    "            print(i+1)\n",
    "            break\n"
